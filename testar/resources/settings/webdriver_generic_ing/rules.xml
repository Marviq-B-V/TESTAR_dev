<?xml version="1.0" encoding="UTF-8"?>
<map>
   <array key="filter">
      <map>
         <array key="xquery">
            <string>ancestor::*[@data-tag-name = 'ing-feat-moir-intro'] and .[not(@id = 'startFlow')]</string>
         </array>
         <array key="doc">
            <string>don't login in</string>
         </array>
      </map>
      <map>
         <array key="xquery">
            <string>ancestor::*[@slot = 'details']</string>
         </array>
         <array key="doc">
            <string>don't consider changing details</string>
         </array>
      </map>
      <map>
         <array key="xquery">
            <string>ancestor-or-self::*[@name = 'ing-feat-moir-appointment-confirmation']</string>
         </array>
         <array key="doc">
            <string>don't confirm appointment</string>
         </array>
      </map>
   </array>
   <array key="input">
      <map>
         <array key="regexp">
            <string>[1-7][0-9]{4}</string>
         </array>
         <array key="xquery">
            <string>ancestor::*[contains(lower-case(@label), 'yearly amount')]</string>
         </array>
         <array key="doc">
            <string>yearly amount</string>
         </array>
         <string key="type">generic</string>
         <array key="priority">
            <string>5</string>
         </array>
      </map>
      <map>
         <array key="regexp">
            <string>(0415580365)|(0[0-9]{9})</string>
         </array>
         <string key="name">enterpriseNumber</string>
         <array key="doc">
            <string>enterprise number</string>
         </array>
         <string key="type">input</string>
         <array key="priority">
            <string>5</string>
         </array>
      </map>
      <map>
         <array key="regexp">
            <string>[1-9][0-9]{4}</string>
         </array>
         <string key="form">income</string>
         <string key="name">years[]</string>
         <array key="doc">
            <string>year income</string>
         </array>
         <string key="type">formInput</string>
         <array key="priority">
            <string>5</string>
         </array>
      </map>
   </array>
   <array key="priority">
      <map>
         <array key="xquery">
            <string>.[not(@type = 'submit')] and</string>
            <string>not(ancestor-or-self::*[h:is-invalid(.)]) and </string>
            <string>tst:visits[@count &gt; 0]</string>
         </array>
         <array key="doc">
            <string>if action visited and not submit or invalid</string>
         </array>
         <array key="priority">
            <string>1 div (1 + count(tst:visits))</string>
         </array>
      </map>
      <map>
         <array key="xquery">
            <string>h:is-radio(.) and ancestor::*[h:is-radiogroup(.) and h:is-invalid(.)]</string>
         </array>
         <array key="doc">
            <string>weighted radio button</string>
         </array>
         <array key="priority">
            <string>3.0</string>
         </array>
      </map>
      <map>
         <array key="xquery">
            <string>.[@type = 'submit']</string>
         </array>
         <array key="doc">
            <string>submit button has higher priority</string>
         </array>
         <array key="priority">
            <string>1 + count(ancestor::form//*[(h:radiogroup-is-empty(.) or </string>
            <string>h:text-is-empty(.) or h:select-is-empty(.)) and h:is-valid(.)])</string>
         </array>
      </map>
   </array>
</map>
